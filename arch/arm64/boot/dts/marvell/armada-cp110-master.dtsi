/*
 * Device Tree file for Marvell Armada CPN-110 Master.
 */

/ {
	cpn-110-master {
		#address-cells = <2>;
		#size-cells = <2>;
		compatible = "simple-bus";
		interrupt-parent = <&gic>;
		ranges;

		config-space {
			#address-cells = <1>;
			#size-cells = <1>;
			compatible = "simple-bus";
			ranges =<0x0 0x0 0xf2000000 0x2000000>;

			#include "armada-cp110.dtsi"
		};
	};
};
